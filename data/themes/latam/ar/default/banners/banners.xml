  <layout name="banner.traffic" width="300" height="64">
    <border resizeX="false" resizeY="false" minY="64"/>
    <background type="image" bitmap="banner.bknd">
      <resize type="frame" left="120" top="0" right="0" bottom="0"/>
    </background>
    <widgets>

      <label id="lab.name"
             anchor-tl="top-left" anchor-br="top-right"
             left="35" top="23" width="86" height="36"
             align="left-center"
             multiline="false"
             ellipsis="false"
             font="font.banner"
             color="#FFFFFF"
             shadow="false"
             text="#str.traffic"/>

      <widgetex class="advertising"
            id="adv1" params="service"
            anchor-tl="top-left" anchor-br="none"
