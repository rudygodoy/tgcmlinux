  <layout name="dock.advertising" width="310" height="60" stickyX="0" stickyY="0" >
    <border resizeX="false" resizeY="false" minX="310" minY="60" />
    <caption type="none" maximize="false" minimize="false"/>
    <dock left="false" right="false" top="false" bottom="false" />
    <background type="image" bitmap="dock.bkgnd2" />
	<shape type="multipart">
		<!-- <part type="roundrect" left="0" top="0" width="310" height="60" rwidth="10" rheight="10"
				anchor-tl="top-right" anchor-br="none" />
		<part type="rect" left="0" top="0" width="310" height="20" join="or"
				anchor-tl="top-right" anchor-br="none" /> -->
		<part type="rect" left="0" top="0" width="310" height="60" join="or"
				anchor-tl="top-right" anchor-br="none" /> -->
	</shape>
    <widgets>
     <widgetex class="advertising"
            id="adv1" params="main"
            anchor-tl="top-right" anchor-br="none"
            left="5" top="5" width="300" height="50" />
    </widgets>
  </layout>
  

  <layout name="dock.layout" width="800" height="102" stickyX="0" stickyY="0" contextmenu="wnd.contextmenu()">
    <border resizeX="true" resizeY="false" minX="830" minY="102" left="5" top="0" right="5" bottom="0"/>
    <caption type="window" top="0" maximize="false" minimize="true"/>
    <dock left="false"
      right="false" top="true" bottom="true"
      autohide="true" keepsize="true"
      image="dock.docking" frames="16" fps="32"/>
    <background type="image" bitmap="dock.bkgnd1">
      <resize type="frame" left="420" top="32" right="378" bottom="10"/>
    </background>
    <!-- <shape type="alphacolor" alphacolor="#FF00FF"/> -->
    <shape type="rect"/>
	<!-- <shape type="multipart">
		<part type="roundrect" left="0" top="0" width="800" height="100" rwidth="18" rheight="18" 
				anchor-tl="top-left" anchor-br="top-right"/>
		<part type="roundrect" left="429" top="80" width="310" height="80" rwidth="10" rheight="10" join="or"
				anchor-tl="top-right" anchor-br="none" />
	</shape> -->
    <accelerators>
        <accelerator key="ctrl+D" action="app.disconnect()"/>
		<accelerator key="ctrl+C" action="app.connect()"/>
		<accelerator key="alt+X" action="window.close()"/>
    </accelerators>	
    <widgets>

      <animate id="anim.logo"
               image="dock.logo"
               left="8" top="0" width="32" height="28"
               anchor-tl="top-left" anchor-br="none"
               frames="57"
               fps="15"
               play="app.connecting or wwan.device.init" />

      <button id="btn.menudev"
              enable="true"
              anchor-tl="top-left" anchor-br="none"
              left="50" top="8" width="130" height="16"
              ondown="app.menudevice()"
              tooltip="#wwan.device.description"
              image-normal="dock.device.normal"
              image-hot="dock.device.hot"
              image-down="dock.device.down"
              image-disable="dock.device.disable"
              text-align="left-center"
              text-left="10"
              text-right="20"
              multiline="false"
