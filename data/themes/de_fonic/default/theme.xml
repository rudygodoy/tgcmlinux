  <layout name="dock.layout" width="719" height="100" stickyX="0" stickyY="0" contextmenu="wnd.contextmenu()">
    <border resizeX="true" resizeY="false" minX="719" minY="100" left="5" top="0" right="5" bottom="0"/>
    <caption type="window" top="0" maximize="false" minimize="true"/>
    <dock left="false"
      right="false" top="true" bottom="true"
      autohide="true" keepsize="true"
      image="dock.docking" frames="16" fps="32"/>
    <background type="image" bitmap="dock.bkgnd">
      <resize type="frame" left="464" top="32" right="20" bottom="8"/>
    </background>
    <!-- <shape type="alphacolor" alphacolor="#FF00FF"/> -->
    <shape type="roundrect" rwidth="18" rheight="18"/>
    <accelerators>
        <accelerator key="ctrl+D" action="app.disconnect()"/>
		<accelerator key="ctrl+C" action="app.connect()"/>
		<accelerator key="alt+X" action="window.close()"/>
    </accelerators>	
    <widgets>

      <animate id="anim.logo"
               image="dock.logo"
               left="9" top="4" width="20" height="20"
               anchor-tl="top-left" anchor-br="none"
               frames="2"
               fps="1"
               play="app.connecting or wwan.device.init" />

      <button id="btn.menudev"
              enable="true"
              anchor-tl="top-left" anchor-br="none"
              left="37" top="8" width="130" height="16"
              ondown="app.menudevice()"
              tooltip="#wwan.device.description"
              image-normal="dock.device.normal"
              image-hot="dock.device.hot"
              image-down="dock.device.down"
              image-disable="dock.device.disable"
              text-align="center-center"
              multiline="false"
              ellipsis="true"
              font-normal="font.normal"
              color-normal="#000000"
              text="#wwan.device"/>

     <button id="btn.prepayurl"
              visible="user.prepay"
              anchor-tl="top-right" anchor-br="none"
              left="400" top="8" width="180" height="16"
              cursor="hand"
              onclick="app.prepaypage()"
              font-normal="font.underline"
              color-normal="#FFFFFF"
              font-hot="font.underline"
              font-down="font.underline"
              text="#str.prepaypage"
              tooltip="#str.prepaypage.description"/>
      

      <button id="btn.traffic"
              anchor-tl="top-right" anchor-br="top-right"
              left="356" top="8" width="260" height="16"
              onclick="app.traffic()"
              tooltip="#user.data.description"
              image-normal="dock.traffic.normal"
              image-hot="dock.traffic.hot"
              image-down="dock.traffic.down"
              image-disable="dock.traffic.disable"
                visible="not user.prepay and traffic.available">
              <resize type="frame" left="10" right="10" top="0" bottom="0"/>
      </button>
	  
      <label id="lab.traffic"
              anchor-tl="top-right" anchor-br="top-right"
              left="362" top="8" width="196" height="16"
              align="left-center"
              multiline="false"
              ellipsis="true"
              font="font.normal"
              color="#FFFFFF"
              text="#(str.user.data.used.label): #str.user.data.used.data / #str.user.data.limit.data"
              visible="not user.prepay and traffic.available"/>

      <progress id="prog.traffic"
                status="user.data.used"
                hittest="transparent"
                anchor-tl="top-right" anchor-br="none"
                left="558" top="8" width="54" height="15"
                visible="not user.prepay and traffic.available">
        <state image="dock.traffic0"/>
        <state image="dock.traffic1"/>
        <state image="dock.traffic2"/>
        <state image="dock.traffic3"/>
        <state image="dock.traffic4"/>
        <state image="dock.traffic5"/>
        <state image="dock.traffic6"/>
        <state image="dock.traffic7"/>
        <state image="dock.traffic8"/>
        <state image="dock.traffic9"/>
        <state image="dock.traffic10"/>
      </progress>

      <button id="btn.help"
              cursor="arrow"
              onclick="app.help()"
              anchor-tl="top-right" anchor-br="none"
              left="650" top="6" width="17" height="17"
              tooltip="#str.help"
              image-normal="wnd.help.normal"
              image-hot="wnd.help.hot"
              image-down="wnd.help.down"/>

      <button id="btn.settings"
