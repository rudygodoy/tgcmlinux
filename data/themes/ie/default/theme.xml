             ellipsis="false"
             font="font.bold"
             color="#FFFFFF"
             text="#str.apptitle"/>


      <button id="btn.traffic"
              visible="not user.prepay and traffic.available"
              anchor-tl="top-right" anchor-br="none"
              left="250" top="6" width="220" height="16"
              cursor="hand"
              onclick="app.traffic()"
              tooltip="#user.data.description"
              image-normal="dock.traffic.normal"
              image-hot="dock.traffic.hot"
              image-down="dock.traffic.down"
              image-disable="dock.traffic.disable"
              text-align="right-center"
              color-normal="#FFFFFF"
              font-normal="font.underline"
              font-hot="font.underline"
              font-down="font.underline"
              multiline="false"
              ellipsis="true"
              text="#(str.user.data.used.label): #str.user.data.used.data / #str.user.data.limit.data"/>
             
      <button id="btn.prepayurl"
              visible="user.prepay"
