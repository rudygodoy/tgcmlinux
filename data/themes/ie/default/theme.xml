        <state image="dock.traffic4"/>
        <state image="dock.traffic5"/>
        <state image="dock.traffic6"/>
        <state image="dock.traffic7"/>
        <state image="dock.traffic8"/>
        <state image="dock.traffic9"/>
        <state image="dock.traffic10"/>
      </progress>

